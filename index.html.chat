<script type="text/javascript">
	function setCurruent(location) {
	    $('ul.ulnav li:not(.active) a').each(function(){
	        var loc = $(this).attr('href');
	        if (loc.indexOf(location) !== -1)
	        {
	            $(this).parent().addClass('active');
	        } else {
	            $(this).parent().removeClass('active');
	        }
	    });
	}
	$(function(){
		  
	    //footer标签定位
	    switch (true) {
			case location.pathname.indexOf('/mobileMoreIntroduc/introduc')!==-1:
			case location.pathname.indexOf('page=newsDetail')!==-1:
			case location.pathname.indexOf('/mobileMoreIntroduc/news')!==-1:
			{
				setCurruent('/mobileMoreIntroduc/introduc');
				break;
			}
			case location.pathname.indexOf('/mobileMoreIntroduc/findnotices')!==-1:
			case location.pathname.indexOf('page=notice')!==-1:
			case location.pathname.indexOf('/mobileMoreIntroduc/find')!==-1:
			{
				setCurruent('/mobileMoreIntroduc/findnotices');
				break;
			}
			case location.pathname.indexOf('/mobileDebt')!==-1:
			case location.pathname.indexOf('/mobileLoan')!==-1:
			
			{
				setCurruent('/mobileLoan/loans');
				break;
			}
			
			case location.pathname.indexOf('/mobileMember')!==-1:
			case location.pathname.indexOf('/mobileBorrow')!==-1:
			case location.pathname.indexOf('/mobileVisitor/to-login')!==-1:
			case location.pathname.indexOf('/mobileSmartInvestment')!==-1:
			case location.pathname.indexOf('/investmentRecord')!==-1:
			{
				setCurruent('/mobileMember/member_center');
				break;
			}
			default:
				setCurruent('/mobileVisitor/index');
				break;
			}
});
</script>
